pip --no-cache-dir install jupyter_nbextensions_configurator
jupyter nbextensions_configurator enable --sys-prefix

jupyter nbextension install --py lc_notebook_diff --sys-prefix
jupyter nbextension enable --py lc_notebook_diff --sys-prefix

pip install https://github.com/NII-cloud-operation/Jupyter-LC_nblineage/tarball/master

jupyter nblineage quick-setup --sys-prefix
